{
  "manifest_version": 3,
  "name": "TabReducer",
  "description": "Reduce tab clutter by closing or saving tabs to categorized reading lists.",
  "version": "1.0",
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}